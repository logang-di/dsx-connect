## Google Cloud Storage Connector - local dev env

# Serve connector over HTTPS (optional)
DSXCONNECTOR_USE_TLS=false
DSXCONNECTOR_TLS_CERTFILE=shared/deploy/certs/dev.localhost.crt
DSXCONNECTOR_TLS_KEYFILE=shared/deploy/certs/dev.localhost.key

# Connector URL and dsx-connect URL
DSXCONNECTOR_CONNECTOR_URL=http://google-cloud-storage-connector:8595
DSXCONNECTOR_DSX_CONNECT_URL=http://dsx-connect-api:8586

# Outbound TLS verification when calling dsx-connect
DSXCONNECTOR_VERIFY_TLS=false
# DSXCONNECTOR_CA_BUNDLE=shared/deploy/certs/dev.localhost.crt

# Connector-specific settings
DSXCONNECTOR_ASSET=your-gcs-bucket
DSXCONNECTOR_FILTER=
