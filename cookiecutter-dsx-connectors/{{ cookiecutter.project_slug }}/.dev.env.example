# Copy this file to `.dev.env` (same directory) to override settings locally while developing.
# Any DSXCONNECTOR_* variables defined here take precedence over config.py defaults.

LOG_LEVEL=debug
DSXCONNECTOR_CONNECTOR_URL=http://127.0.0.1:{{ cookiecutter.connector_port }}
DSXCONNECTOR_DSX_CONNECT_URL={{ cookiecutter.__dsx_connect_url }}

# TLS options when running locally (disable verification against self-signed certificates)
DSXCONNECTOR_VERIFY_TLS=false
# DSXCONNECTOR_CA_BUNDLE=/path/to/ca.pem

# Optional friendly metadata for the dsx-connect UI cards
# DSXCONNECTOR_DISPLAY_NAME={{ cookiecutter.project_name }}
# DSXCONNECTOR_DISPLAY_ICON=data:image/svg+xml;utf8,<svg .../>

# Repository-specific overrides
DSXCONNECTOR_ASSET=
DSXCONNECTOR_FILTER=

# Uncomment and add additional connector-specific settings below as needed.
